<style>
    #cta {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        justify-content: center;
        align-items: center;
    }

    #cta .book-container {
        display: flex;
        padding: 120px 15%;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        background-color: #0a2046;
    }

    #cta .book-now {
        display: flex;
        max-width: 620px;
        flex-direction: column;
        justify-content: center;
        /* align-items: flex-start; */
    }

    #cta .book-now h4 {
        color: #27b1d9;
        font-size: 16px;
        line-height: 1.2em;
        font-weight: 700;
    }

    #cta .book-now h2 {
        margin-top: 0px;
        margin-bottom: 30px;
        font-family: 'DM Serif Display', sans-serif;
        color: #fff;
        font-size: 52px;
        line-height: 1.15em;
        font-weight: 400;
    }

    #cta .book-now p {
        margin-bottom: 35px;
        color: #9da5b5;
    }

    #cta .book-now .contact-form {
        max-width: 510px;
        margin: 0 0 15px;
        display: grid;
        align-items: center;
        grid-auto-columns: 1fr;
        column-gap: 10px;
        row-gap: 15px;
        grid-template-columns: 2fr 1fr;
        grid-template-rows: auto;
    }

    #cta .book-now .contact-form input {
        height: 58px;
        margin-bottom: 0px;
        padding: 20px 25px;
        border-style: solid;
        border-width: 1px;
        border-color: #caebed;
        border-radius: 100px;
        background-color: #fff;
        transition: border-color 300ms cubic-bezier(.215, .61, .355, 1);
        color: #9da5b5;
        font-size: 17px;
        line-height: 1.2em;
        font-weight: 600;
        outline: none;
    }

    #cta .video1,
    #cta .video2 {
        position: relative;
        overflow: hidden;
        height: 500px;
        color: white;
        width: 100%;
        height: 100%;
        min-height: 600px;
        -o-object-fit: cover;
        object-fit: cover;
    }

    #cta .video1 video,
    #cta .video2 video {
        background-size: cover;
        background-position: 50% 50%;
        position: absolute;
        margin: auto;
        width: 100%;
        height: 100%;
        right: -100%;
        bottom: -100%;
        top: -100%;
        left: -100%;
        object-fit: cover;
        z-index: -100;
    }

    #cta .why {
        display: flex;
        padding: 120px 15%;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        background-color: #eaf9fa;
    }

    #cta .why .content {
        display: flex;
        max-width: 620px;
        flex-direction: column;
        justify-content: center;
        align-items: flex-start;
    }

    #cta .why .content h4 {
        color: #27b1d9;
        font-size: 16px;
        line-height: 1.2em;
        font-weight: 700;
    }

    #cta .why .content h2 {
        margin-top: 0px;
        margin-bottom: 30px;
        font-family: 'DM Serif Display', sans-serif;
        color: #0a2046;
        font-size: 52px;
        line-height: 1.15em;
        font-weight: 400;
    }

    #cta .why .content ul {
        width: 100%;
    }

    #cta .why .content ul li {
        margin-top: 0px;
        margin-bottom: 10px;
        font-family: 'Nunito', sans-serif;
        color: #0a2046;
        font-size: 18px;
        line-height: 1.45em;
        font-weight: 700;
        display: flex;
        align-items: center;
        gap: 10px;

    }

    #cta .why .content ul li span:first-child {
        display: flex;
        width: 33px;
        height: 33px;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        flex: 0 0 auto;
        border-radius: 100px;
        background-color: rgba(39, 177, 217, 0.1);
    }
</style>

